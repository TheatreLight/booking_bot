@model IEnumerable<WebApplication_Vitalik.Models.Booking>

@{
    ViewData["Title"] = "Index";
    Layout = "_LayoutInternal";
}

<h2>Список бронирования</h2>
<p><a href="#" disabled="disabled">Добавить объект</a></p>
<table class="table table-striped table-bordered table-hover">
    <tr><th>Объект</th><th>Пользователь</th><th>Дата</th><th>С</th><th>По</th><th></th><th></th></tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.SubjectId</td>
            <td>@item.UserId</td>
            <td>@item.Date</td>
            <td>@item.TimeFrom</td>
            <td>@item.TimeTo</td>
            <td>
                <a class="btn btn-default" href="#" disabled="disabled">Изменить</a>
            </td>
            <td>
                <form method="post">
                    <input class="btn btn-danger" type="button" disabled="disabled" value="Удалить" />
                </form>
            </td>
        </tr>
    }
</table>